{
  "name": "nestjs-prisma--backend",
  "version": "0.0.1",
  "description": "nestjs-prisma--backend",
  "author": "shamilfrontend",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postinstall": "prisma generate && yarn build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "db:stage:migrate:status": "prisma migrate status",
    "db:stage:migrate:deploy": "prisma migrate deploy",
    "start:stage": "node dist/main"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/cqrs": "^7.0.1",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/swagger": "^4.8.0",
    "@prisma/client": "^2.24.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "date-fns": "^2.21.3",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@qvant/prettier-config": "^0.0.1",
    "@types/express": "^4.17.11",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^14.14.36",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "prisma": "^2.30.0",
    "supertest": "^6.1.3",
    "ts-loader": "^8.0.18",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "prisma": {
    "schema": "src/Prisma/schema.prisma"
  }
}
